<icepack>

<server name="${name}" kind="java-icebox" endpoints="default" activation="on-demand">

  <variable name="inheritedvar" value="inherited"/>

  <include name="Service1" descriptor="service.xml"/>
  <include name="Service2" descriptor="freezeservice.xml"/>

  <properties>
    <property name="Ice.Warn.Connections" value="1"/>
  </properties>

</server>

</icepack>
