<service kind="standard" entry="Service">

  <adapters>
    <adapter name="${name}" endpoints="default" id="${name}Adapter">
      <offer interface="::Test" identity="${name}"/>
    </adapter>
  </adapters>

  <properties>
    <property name="${name}.Type" value="standard"/>
    <property name="${name}.ServiceName" value="${name}"/>
  </properties>

  <target name="debug">
    <properties>
      <property name="${name}.DebugProperty" value="debug"/>
    </properties>
  </target>

</service>